# UAT Environment - Chat Commerce Web
VITE_APP_NAME=WeedGo Chat Commerce (UAT)
VITE_ENVIRONMENT=uat

# API Configuration
VITE_API_URL=https://weedgo-uat.koyeb.app
VITE_API_BASE_URL=https://weedgo-uat.koyeb.app/api
VITE_WS_URL=wss://weedgo-uat.koyeb.app
VITE_SOCKET_URL=wss://weedgo-uat.koyeb.app

# Chat Configuration
VITE_ENABLE_VOICE_CHAT=true
VITE_ENABLE_VIDEO_CHAT=false
VITE_MAX_MESSAGE_LENGTH=2000

# Features
VITE_ENABLE_CHAT_HISTORY=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_EMOJI=true
VITE_ENABLE_TYPING_INDICATOR=true

# Monitoring
VITE_SENTRY_DSN=https://xxx@sentry.io/uat-chat
VITE_ENV_LABEL=UAT
