# iOS Simulator Configuration
# Use this configuration when testing on iOS Simulator

# API Configuration - Using localhost for iOS Simulator
EXPO_PUBLIC_API_URL=http://localhost:5024
EXPO_PUBLIC_API_TIMEOUT=30000
EXPO_PUBLIC_TENANT_ID=ce2d57bc-b3ba-4801-b229-889a9fe9626d

# WebSocket URLs
EXPO_PUBLIC_WS_URL=ws://localhost:5024
EXPO_PUBLIC_VOICE_WS_URL=ws://localhost:5024/api/v2/ai-conversation/ws

# Template Configuration
EXPO_PUBLIC_TEMPLATE=pot-palace

# Feature Flags
EXPO_PUBLIC_ENABLE_VOICE=true
EXPO_PUBLIC_ENABLE_CHAT=true
EXPO_PUBLIC_ENABLE_BIOMETRIC=false  # Biometric not available in simulator

# Environment
EXPO_PUBLIC_ENV=development

# Analytics (disabled for simulator testing)
EXPO_PUBLIC_ANALYTICS_ENABLED=false