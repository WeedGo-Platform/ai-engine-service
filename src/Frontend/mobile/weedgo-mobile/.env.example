# API Configuration
# Copy this file to .env.local and update with your settings

# Backend API URL (update with your machine's IP address)
EXPO_PUBLIC_API_URL=http://10.0.0.169:5024

# WebSocket URL (usually same host as API)
EXPO_PUBLIC_WS_URL=ws://10.0.0.169:5024

# Voice WebSocket URL
EXPO_PUBLIC_VOICE_WS_URL=ws://10.0.0.169:5024/api/voice/ws/stream

# Tenant Configuration
EXPO_PUBLIC_TENANT_ID=ce2d57bc-b3ba-4801-b229-889a9fe9626d

# API Timeout (milliseconds)
EXPO_PUBLIC_API_TIMEOUT=30000

# Environment
EXPO_PUBLIC_ENV=development

# Features
EXPO_PUBLIC_ENABLE_VAD=true
EXPO_PUBLIC_ENABLE_WAKE_WORD=true
EXPO_PUBLIC_ENABLE_AUTO_STOP=true
