# Production Configuration
# Use this configuration for production builds

# API Configuration - Production server
# Update with your actual production API URL
EXPO_PUBLIC_API_URL=https://api.weedgo.ca
EXPO_PUBLIC_API_TIMEOUT=30000
EXPO_PUBLIC_TENANT_ID=ce2d57bc-b3ba-4801-b229-889a9fe9626d

# WebSocket URLs - Production
EXPO_PUBLIC_WS_URL=wss://api.weedgo.ca
EXPO_PUBLIC_VOICE_WS_URL=wss://api.weedgo.ca/api/v2/ai-conversation/ws

# Template Configuration
EXPO_PUBLIC_TEMPLATE=pot-palace

# Feature Flags - Production settings
EXPO_PUBLIC_ENABLE_VOICE=true
EXPO_PUBLIC_ENABLE_CHAT=true
EXPO_PUBLIC_ENABLE_BIOMETRIC=true

# Environment
EXPO_PUBLIC_ENV=production

# Analytics - Enable for production
EXPO_PUBLIC_ANALYTICS_ENABLED=true
EXPO_PUBLIC_SENTRY_DSN=your-sentry-dsn-here

# Map Configuration - Production
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your-production-google-maps-key

# Payment Configuration - Production
EXPO_PUBLIC_CLOVER_SANDBOX=false
EXPO_PUBLIC_CLOVER_APP_ID=your-production-clover-app-id